{
  "version": 2,
  "builds": [
    {
      "src": "geojson-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "geojson-backend/src/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/geojson-backend/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/geojson-frontend/build/$1"
    }
  ]
}
